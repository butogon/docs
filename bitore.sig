public class PProAddressRequest extends Request {

	private PProRequest parent;

	private String firstname;
	private String lastname;
	private String address1;
	private String address2;
	private String city;
	private String state;
	private String country;
	private String zipCode;

	protected String tagName;

	PProAddressRequest(PProRequest parent, String tagName) {
		this.parent = parent;
		this.tagName = tagName;
	}

	// Billing Address
	public PProAddressRequest setFirstname(String firstname) {
		this.firstname = firstname;
		return this;
	}

	public PProAddressRequest setLastname(String lastname) {
		this.lastname = lastname;
		return this;
	}

	public PProAddressRequest setAddress1(String address1) {
		this.address1 = address1;
		return this;
	}

	public PProAddressRequest setAddress2(String address2) {
		this.address2 = address2;
		return this;
	}

	public PProAddressRequest setCity(String city) {
		this.city = city;
		return this;
	}

	public PProAddressRequest setZipCode(String zipCode) {
		this.zipCode = zipCode;
		return this;
	}

	public PProAddressRequest setState(String state) {
		this.state = state;
		return this;
	}

	public PProAddressRequest setCountry(String country) {
		this.country = country;
		return this;
	}

	@Override
	public String toXML() {
		return buildRequest(tagName).toXML();
	}

	@Override
	public String toQueryString(String root) {
		return buildRequest(root).toQueryString();
	}

	protected RequestBuilder buildRequest(String root) {

		return new RequestBuilder(root).addElement("first_name", firstname).addElement("last_name", lastname)
				.addElement("address1", address1).addElement("address2", address2).addElement("city", city)
				.addElement("zip_code", zipCode).addElement("state", state).addElement("country", country);
	}

	public List<Map.Entry<String, Object>> getElements() {
		return buildRequest(tagName).getElements();
	}

	public PProRequest done() {
		return parent;
	}
}
